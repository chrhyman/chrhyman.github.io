{
  "model": {
    "type": "select",
    "property": "custom_model_data",
    "cases": [
      {
        "when": "trophy_most_diamonds",
        "model": {
          "type": "model",
          "model": "mw2:item/trophy/most_diamonds"
        }
      },
      {
        "when": "trophy_end_explosion",
        "model": {
          "type": "model",
          "model": "mw2:item/trophy/end_explosion"
        }
      }
    ],
    "fallback": {
      "type": "model",
      "model": "minecraft:block/carved_pumpkin"
    }
  }
}
